                </cubepl>
                <cubeplinit>
                {
                    global(mpi);
                    ${i} = 0;
                    while ( ${i} < ${cube::#callpaths} )
                    {
                        ${mpi}[${i}] = 0;
                        ${regionid} = ${cube::callpath::calleeid}[${i}];
                        if ( ${cube::region::paradigm}[${regionid}] eq "mpi" )
                        {
                            ${mpi}[${i}] = 1;
                        };
                        ${i} = ${i} + 1;
                    };
                    return 0;
                }
                </cubeplinit>
